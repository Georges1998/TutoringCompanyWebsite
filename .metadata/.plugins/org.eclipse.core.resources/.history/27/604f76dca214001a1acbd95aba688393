package ca.mcgill.ecse321.eventregistration.dto;

public class CreditCardDto {
	private String ID;
	private int amount;

	public CreditCardDto(String ID, int amount) {
		this.ID = ID;
		this.amount = amount;
	}


	public set setEvent(String ID) {
		this.ID = ID;
	}

	public PersonDto getPerson() {
		return person;
	}

	public void setPerson(PersonDto person) {
		this.person = person;
	}

}


