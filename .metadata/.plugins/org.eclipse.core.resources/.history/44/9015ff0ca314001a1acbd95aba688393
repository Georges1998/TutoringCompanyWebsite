package ca.mcgill.ecse321.eventregistration.dto;

public class CreditCardDto {
	private String ID;
	private int amount;

	public CreditCardDto(String ID, int amount) {
		this.ID = ID;
		this.amount = amount;
	}


	public void setID(String ID) {
		this.ID = ID;
	}

	public String getID() {
		return ID;
	}

	public void setAmount(int amount) {
		this.amount = amount;
	}

	public int getID() {
		return ID;
	}

}


